<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="palrestaurant.emm.pal_restaurant">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Registro" />
        <activity android:name=".Registrar_Comensal" />
        <activity android:name=".Subir_Menu" />
        <activity android:name=".activity_registrar_restaurante" />
        <activity android:name=".activity_actualizar_restaurante" />
        <activity android:name=".activity_responder_encuesta" />
        <activity
            android:name=".perfilRestaurante"
            android:label="@string/title_activity_perfil_restaurante"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>